         <ChoiceBox prefWidth="150.0" onAction="#musicChoices">
             <items>
                 <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Song 1" />
                    <String fx:value="Song 2" />
                     <String fx:value="Song 3" />
                </FXCollections>
             </items>
         </ChoiceBox>
         <Slider fx:id="vslider" layoutX="26.0" layoutY="32.0" majorTickUnit="50" max="200" minorTickCount="0" showTickLabels="true" showTickMarks="true" />
      </children>
      <padding>
         <Insets top="100.0" />
      </padding>
   </VBox>
    <Button fx:id="backToMainMenuButton" layoutX="676.0" layoutY="546.0" mnemonicParsing="false" onAction="#openMainMenuScene" text="Back To Menu" />

    <Button layoutX="550.0" layoutY="546.0" mnemonicParsing="false" onAction="#backToSettings" text="Back to Settings" />
    <Text layoutX="321.0" layoutY="89.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sound Settings">
      <font>
          <Font size="25.0" />
      </font>
   </Text>

